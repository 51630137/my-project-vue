<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jg.myprojectvue.mapper.DepartmentMapper">
    <resultMap id="BaseResultMap" type="com.jg.myprojectvue.pojo.Department">
        <id property="id" column="id"/>
        <id property="name" column="name"/>
    </resultMap>

    <select id="departmentLst" resultType="com.jg.myprojectvue.pojo.Department">
        select
        id,name
        from department
    </select>
</mapper>